{% extends 'base.html.twig' %}

{% block title %}Extension{% endblock %}

{% block body %}
    <h1>Extension</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ extension.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ extension.nom }}</td>
            </tr>
            <tr>
                <th>ApkName</th>
                <td>{{ extension.apkName }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ extension.updatedAt ? extension.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_extension_index') }}">back to list</a>

    <a href="{{ path('app_extension_edit', {'id': extension.id}) }}">edit</a>

    {{ include('extension/_delete_form.html.twig') }}
{% endblock %}
